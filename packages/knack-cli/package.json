{
	"name": "@optum/knack-cli",
	"version": "1.3.0",
	"description": "A cli for working with Apache Kafka development.",
	"license": "Apache-2.0",
	"licenses": [
		{
			"type": "Apache-2.0",
			"url": "http://www.apache.org/licenses/LICENSE-2.0"
		}
	],
	"repository": "optum/knack",
	"author": {
		"name": "lxghtless",
		"email": "william.kurth@optum.com",
		"url": "https://github.com/lxghtless"
	},
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=10.18.0 <11 || >=12.14.0 <13 || >=13.5.0"
	},
	"scripts": {
		"start": "node cli.js --help",
		"lint": "xo --fix"
	},
	"bin": {
		"knack": "./cli.js"
	},
	"files": [
		"index.js",
		"cli.js",
		"src"
	],
	"keywords": [
		"kafka"
	],
	"devDependencies": {
		"xo": "^0.29.1"
	},
	"nyc": {
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"rules": {
			"unicorn/catch-error-name": [
				"error",
				{
					"caughtErrorsIgnorePattern": "^_$"
				}
			],
			"unicorn/filename-case": [
				"error",
				{
					"case": "camelCase"
				}
			],
			"capitalized-comments": 0
		}
	},
	"dependencies": {
		"@optum/knack-avro": "^1.2.0",
		"@optum/knack-consumer-client": "^1.2.0",
		"@optum/knack-producer": "^1.0.0",
		"@optum/knack-producer-client": "^1.2.0",
		"@optum/knack-sr": "^1.0.0",
		"avsc": "^5.4.18",
		"chalk": "^2.4.2",
		"clean-stack": "^2.2.0",
		"fs-extra": "^8.1.0",
		"meow": "^5.0.0",
		"node-rdkafka": "^2.7.4",
		"ora": "^3.4.0",
		"p-pipe": "^3.0.0",
		"ramda": "^0.27.0"
	}
}
