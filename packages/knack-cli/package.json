{
  "name": "knack-cli",
  "version": "0.0.0",
  "description": "A cli for working with Apache Kafka development.",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "repository": "optum/knack",
  "author": {
    "name": "lxghtless",
    "email": "william.kurth@optum.com",
    "url": "github.com/lxghtless"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "xo && nyc ava"
  },
  "bin": {
    "knack": "./cli.js"
  },
  "files": [
    "index.js",
    "cli.js",
    "cmds"
  ],
  "keywords": [
    "kafka"
  ],
  "devDependencies": {
    "ava": "^2.2.0",
    "lodash": ">=4.17.13",
    "lodash.merge": ">=4.6.2",
    "lodash.mergewith": ">=4.6.2",
    "nyc": "^14.1.0",
    "xo": "^0.24.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "xo": {
    "rules": {
      "unicorn/catch-error-name": [
        "error",
        {
          "caughtErrorsIgnorePattern": "^_$"
        }
      ],
      "unicorn/filename-case": [
        "error",
        {
          "case": "camelCase"
        }
      ],
      "capitalized-comments": 0
    }
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "fs-extra": "^8.1.0",
    "meow": "^5.0.0",
    "clean-stack": "^2.1.0",
    "knack-avro": "0.0.0",
    "knack-consumer-client": "0.0.0",
    "knack-producer-client": "0.0.0",
    "knack-sr": "0.0.0"
  }
}
